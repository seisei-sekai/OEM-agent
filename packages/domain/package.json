{
  "name": "@repo/domain",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./entities": {
      "types": "./dist/entities/index.d.ts",
      "default": "./dist/entities/index.js"
    },
    "./value-objects": {
      "types": "./dist/value-objects/index.d.ts",
      "default": "./dist/value-objects/index.js"
    },
    "./repositories": {
      "types": "./dist/repositories/index.d.ts",
      "default": "./dist/repositories/index.js"
    },
    "./services": {
      "types": "./dist/services/index.d.ts",
      "default": "./dist/services/index.js"
    },
    "./events": {
      "types": "./dist/events/index.d.ts",
      "default": "./dist/events/index.js"
    }
  },
  "scripts": {
    "build": "tsc --build",
    "clean": "rm -rf dist",
    "type-check": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "dependencies": {
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/node": "^20.10.6",
    "typescript": "^5.3.3",
    "vitest": "^1.2.0",
    "@vitest/ui": "^1.2.0"
  }
}

